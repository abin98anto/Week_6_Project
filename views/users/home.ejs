<% include("../layouts/header.ejs") %>
<h3>Welcome Home! <%= user.name %></h3>
<table width="20%">
  <tr>
    <td>
      <img
        src="userImages/<%= user.image %>"
        width="100px"
        height="100px"
        alt="<%= user.image %>"
      />
    </td>
  </tr>
  <tr>
    <td>Email:</td>
    <td><%= user.email %></td>
  </tr>
  <tr>
    <td>Mobile:</td>
    <td><%= user.mobile %></td>
  </tr>
</table>
<a href="/edit?id=<%= user._id %>">Edit</a>

<a href="/logout">Logout</a>
<% include('../layouts/footer.ejs') %>
